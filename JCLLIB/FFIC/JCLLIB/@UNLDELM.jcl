  TEMPLATE DDPUNCH
     DSN 'DBA5.TEST2.PUN.&DB..&TS.'
     UNIT SYSDA SPACE (1,1) TRK
     DISP (NEW,CATLG,DELETE)
--
  UNLOAD DATA FROM TABLE SYSIBM.SYSPACKAGE HEADER NONE
         (COLLID       CHAR(18),
          NAME         CHAR(18),
          OWNER        CHAR(18),
          CREATOR      CHAR(18),
          QUALIFIER    CHAR(18),
          TIMESTAMP    DATE EXTERNAL,
          BINDTIME     DATE EXTERNAL,
          VALID        CHAR(18),
          OPERATIVE    CHAR(18),
          VALIDATE     CHAR(18),
          ISOLATION    CHAR(18),
          PDSNAME      CHAR(44) )
    WHEN (NAME = 'CDITUCLU')
         PUNCHDDN DDPUNCH
         UNLDDN   DDREC
    EBCDIC DELIMITED
    SHRLEVEL CHANGE ISOLATION UR

